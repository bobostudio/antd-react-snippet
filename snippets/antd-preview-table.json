{
  "Antd PreView Table": {
    "prefix": "apt",
    "body": [
      "/**",
      " *  //TODO ${1: 注释}",
      " */",
      "import { CustomColumnsType, ProTable } from '@metering-system/components'",
      "import { Form } from 'antd'",
      "import { useEffect, useState } from 'react'",
      "",
      "interface InvoiceRecordTableProps {",
      "    selectedRowKeys: string[]",
      "}",
      "",
      "export default function InvoiceRecordTable(props: InvoiceRecordTableProps) {",
      "    const { selectedRowKeys } = props",
      "    const [form] = Form.useForm()",
      "    const [dataSource, setDataSource] = useState<${2:any}[]>([])",
      "",
      "    // 表格列配置",
      "    const columns: CustomColumnsType = [",
      "        //TODO",
      "    ]",
      "",
      "    useEffect(() => {",
      "        const fetchData = async () => {",
      "            // TODO ",
      "            if (selectedRowKeys && selectedRowKeys.length > 0) {",
      "                const res = await ${3: Promise.resolve({ ids: selectedRowKeys \\}) as any}",
      "                if (res && res?.data) {",
      "                    setDataSource(res?.data)",
      "                }",
      "            } else {",
      "                setDataSource([])",
      "            }",
      "        }",
      "        fetchData()",
      "    }, [selectedRowKeys])",
      "",
      "    return (",
      "        <Form form={form} component={false}>",
      "            <ProTable",
      "                //TODO            ",
      "                rowKey='id'",
      "                columns={columns}",
      "                dataSource={dataSource}",
      "                setting={false}",
      "                isShowRowSelection={false}",
      "                isFooter={false}",
      "            />",
      "        </Form>",
      "    )",
      "}"
    ],
    "description": "Antd 预览表格组件模板"
  }
}
